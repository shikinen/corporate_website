  "bugs": {
    "url": "https://github.com/shikinen/corporate_website/issues"
  },
  "homepage": "https://github.com/shikinen/corporate_website#readme",
  "devDependencies": {
    "autoprefixer": "^7.1.4",
    "concat": "^1.0.3",
    "gulp": "^3.9.1",
    "gulp-sass": "^4.0.0",
    "gulp-sourcemaps": "^2.6.0",
    "node-sass": "^4.9.0",
    "npm-run-all": "^4.1.3",
    "postcss-cli": "^4.1.1"
  },
  "dependencies": {
    "svg4everybody": "^2.1.9"
  }
